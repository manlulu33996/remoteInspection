.meeting-res-view{
  padding: 28rpx 28rpx 60rpx 28rpx;
  background: #eee;
  height: 100vh;
  overflow: auto;
}
.tip{
  font-size:24rpx;
  line-height: 60rpx;
  color: #333;
  margin-bottom: 20rpx;
}
.res-li{
  background: #fff;
  padding: 20rpx 28rpx;
  border-radius: 10rpx;
}
.li{
  font-size: 28rpx;
  line-height: 32rpx;
  border-bottom: 1px solid #eee;
  padding: 20rpx 0;
  padding-left: 60rpx;
  position: relative;
}
.li:nth-last-child(1){
  border:none;
}

.li icon{
  position: absolute;
  top: 18rpx;
  left: 0rpx;
}

.success-tip{
 padding: 30rpx 28rpx;
 font-size: 28rpx;
 line-height: 40rpx;
 position: relative;
 padding-left: 60rpx;
 margin-top: 20rpx;
}

.success-tip icon{
  position: absolute;
  top: 28rpx;
  left: 10rpx;
}

.btn{
  width: 300rpx;
  margin: 50rpx auto;
  text-align: center;
  background: #347be7;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  border-radius: 50rpx;
  color: #fff;
}

.pic-list{
  padding: 60rpx 30rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.pic{
  width: 200rpx;
  height: 200rpx;
  position: relative;
  margin-right: 20rpx;
}
.pic-list view:nth-of-type(3n){
  margin-right: 0;
}
.pic .icon{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  background: rgba(0,0,0,.5);
  text-align: center;
  line-height: 36rpx;
  color: #fff;
  border-radius: 50%;
  top: -16rpx;
  right: -12rpx;
}
.pic image{
  width: 200rpx;
  height: 200rpx;
}

.pic-more{
  width: 200rpx;
  height: 200rpx;
  border: 1px dashed #ccc;
  text-align: center;
  line-height: 180rpx;
}
.pic-more text{
  font-size: 120rpx;
  color: #999;
}